{
  "name": "echolocation-audio-sim",
  "version": "1.0.0",
  "private": true,
  "description": "Next.js TypeScript app that simulates movable 3D audio objects for earbuds",
  "packageManager": "npm@10.8.2",
  "engines": {
    "node": ">=20.9.0"
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint . --max-warnings=0",
