    "check": "npm run lint && npm run typecheck && npm run build"
  },
  "dependencies": {
    "next": "16.1.6",
    "react": "19.2.4",
    "react-dom": "19.2.4"
  },
  "devDependencies": {
    "@types/node": "25.3.0",
    "@types/react": "19.2.14",
    "@types/react-dom": "19.2.3",
    "eslint": "9.38.0",
    "eslint-config-next": "16.1.6",
    "typescript": "5.9.3"
  }
}
